# AWS region for Cognito and API Gateway
VITE_AWS_REGION=us-east-1

# Cognito User Pool ID (e.g. us-east-1_XXXXXXXXX)
VITE_COGNITO_USER_POOL_ID=

# Cognito App Client ID (the "client ID" from your User Pool app client)
VITE_COGNITO_CLIENT_ID=

# API Gateway base URL (e.g. https://xxxxxxxxxx.execute-api.us-east-1.amazonaws.com/dev)
VITE_API_BASE_URL=

# Per-tier API Gateway API keys (public rate-limiting keys, not secrets)
VITE_API_KEY_FREE=
VITE_API_KEY_PRO=
VITE_API_KEY_PLUS=
VITE_API_KEY_POWER=

# Stripe price IDs â€” sandbox values below, replace with live IDs for production
VITE_STRIPE_PRICE_PRO_MONTHLY=
VITE_STRIPE_PRICE_PRO_ANNUAL=
VITE_STRIPE_PRICE_PLUS_MONTHLY=
VITE_STRIPE_PRICE_PLUS_ANNUAL=
VITE_STRIPE_PRICE_POWER_MONTHLY=
VITE_STRIPE_PRICE_POWER_ANNUAL=
